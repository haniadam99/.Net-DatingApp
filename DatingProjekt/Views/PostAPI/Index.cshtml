@model IEnumerable<DatingProjekt.Models.Post>

@{
    ViewData["Title"] = "Index";
}

<h1>Inlägg på användarens profil</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Text)
            </th>
            <th>
                Publiceringsdatum
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Text)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublishDate)
            </td>
        </tr>
}
    </tbody>
</table>
